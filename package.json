{
  "name": "regexgen",
  "version": "1.0.0",
  "description": "Simple Regex Generator",
  "keywords": [
    "regex",
    "generator",
    "regexgen"
  ],
  "main": "src/index.tsx",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --host",
    "prepare": "husky install",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-string-replace-recursively": "0.2.0",
    "uniqolor": "1.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/coverage-c8": "^0.30.1",
    "husky": "^8.0.0",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vitest": "^0.30.1"
  }
}
